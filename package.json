{
  "name": "fetchline-monorepo",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test:node": "lerna run test:node",
    "test:deno": "lerna run test:deno",
    "test": "yarn run test:node && yarn run test:deno",
    "clean:nodemodules": "lerna clean",
    "clean:dist": "rimraf packages/**/dist",
    "clean:buildcache": "rimraf packages/**/buildcache",
    "rebuild": "yarn run clean:buildcache && yarn run build",
    "prepublishOnly": "yarn run rebuild && copyreadme",
    "publishall": "lerna publish",
    "copyreadme": "cp README.md packages/fetchline && cp README.md packages/nodefetchline && cp README.md packages/isomorphic-fetchline && cp README.md packages/naivefetchline && cp README.md packages/getfileline && cp README.md packages/readlineiter"
  },
  "devDependencies": {
    "lerna": "^3.22.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ava": "^3.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0"
  },
  "workspaces": [
    "packages/fetchline",
    "packages/nodefetchline",
    "packages/isomorphic-fetchline",
    "packages/naivefetchline",
    "packages/getfileline",
    "packages/readlineiter"
  ]
}
